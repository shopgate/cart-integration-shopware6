{
  "name": "shopgate/cart-integration-shopware6",
  "description": "Shopgate Legacy plugin for Shopware 6",
  "license": "proprietary",
  "type": "shopware-platform-plugin",
  "version": "2.10.2",
  "keywords": [
    "mobile",
    "app"
  ],
  "authors": [
    {
      "name": "Apite GmbH",
      "email": "mail@apite.io"
    }
